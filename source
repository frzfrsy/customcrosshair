local fake_module_scripts = {}

local function PFZPSLS_fake_script() -- Fake Script: Workspace.UserInterface.Custom.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_Custom"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local Custom = script.Parent
	local Apply = script.Parent.Parent.Apply
	local Source = script.Parent.Parent.Source
	local SourceV2 = script.Parent.Parent.SourceV2
	
	local enable = false
	
	Custom.MouseButton1Click:Connect(function()
		if enable == false then
			enable = true
			
			Apply:TweenSizeAndPosition(
				UDim2.fromOffset(32, 32),
				UDim2.new(1, -16, 1, -64),
				Enum.EasingDirection.InOut,
				Enum.EasingStyle.Quint,
				0.5
			)
			
			Source:TweenSizeAndPosition(
				UDim2.fromOffset(100, 32),
				UDim2.new(1, -64, 1, -16),
				Enum.EasingDirection.InOut,
				Enum.EasingStyle.Quint,
				0.5
			)
			
			SourceV2:TweenSizeAndPosition(
				UDim2.fromOffset(100, 32),
				UDim2.new(1, -64, 1, -64),
				Enum.EasingDirection.InOut,
				Enum.EasingStyle.Quint,
				0.5
			)
		else
			enable = false
			
			Apply:TweenSizeAndPosition(
				UDim2.fromOffset(0, 0),
				UDim2.new(1, 0, 1, 0),
				Enum.EasingDirection.InOut,
				Enum.EasingStyle.Quint,
				0.5
			)
	
			Source:TweenSizeAndPosition(
				UDim2.fromOffset(0, 0),
				UDim2.new(1, 0, 1, 0),
				Enum.EasingDirection.InOut,
				Enum.EasingStyle.Quint,
				0.5
			)
			
			SourceV2:TweenSizeAndPosition(
				UDim2.fromOffset(0, 0),
				UDim2.new(1, 0, 1, 0),
				Enum.EasingDirection.InOut,
				Enum.EasingStyle.Quint,
				0.5
			)
		end
	end)
end
local function NHLVDZB_fake_script() -- Fake Script: Workspace.UserInterface.Source.LimitationAndOrientation
    local script = Instance.new("LocalScript")
    script.Name = "LimitationAndOrientation"
    script.Parent = Converted["_Source"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local textBox = script.Parent
	local Apply = textBox.Parent.Apply
	
	local function onlyAllowNumbers()
		textBox.Text = string.gsub(textBox.Text, "%D", "")
	end
	
	local function loadImage()
		local imageId = "rbxassetid://" .. textBox.Text
		textBox.Parent.Cross.Image = imageId
		
		if textBox.Parent.Cross.IsLoaded == false then
			game.StarterGui:SetCore("SendNotification", {
				Title = "Crazitifation",
				Text = "Invalid image ID or failed to load image. ðŸ˜…",
				Duration = 2
			})
			
			return
		end
	end
	
	textBox:GetPropertyChangedSignal("Text"):Connect(onlyAllowNumbers)
	
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
		if #textBox.Text > 10 then
			textBox.Text = string.sub(textBox.Text, 1, 11)
		end
	end)
	
	Apply.MouseButton1Click:Connect(loadImage)
end
local function DUOXN_fake_script() -- Fake Script: Workspace.UserInterface.SourceV2.LimitationAndOrientation
    local script = Instance.new("LocalScript")
    script.Name = "LimitationAndOrientation"
    script.Parent = Converted["_SourceV2"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local textBox = script.Parent
	
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
		if #textBox.Text > 10 then
			textBox.Text = string.sub(textBox.Text, 1, 3)
		end
	end)
	
	textBox.Parent.Apply.MouseButton1Click:Connect(function()
		local txt = textBox.Text
		textBox.Parent.Cross.ImageTransparency = txt
	end)
	
	
end
local function ERDAF_fake_script() -- Fake Script: Workspace.UserInterface.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_UserInterface"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	game.Players.LocalPlayer.Chatted:Connect(function(message, recipient)
		if message == "/destroy UI" then
			script.Parent:Destroy()
		end
	end)
end

coroutine.wrap(PFZPSLS_fake_script)()
coroutine.wrap(NHLVDZB_fake_script)()
coroutine.wrap(DUOXN_fake_script)()
coroutine.wrap(ERDAF_fake_script)()
